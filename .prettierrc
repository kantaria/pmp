  "trailingComma": "all",
  "tabWidth": 2,
  "endOfLine": "lf",
  "singleQuote": true,
  "semi": true,
  "printWidth": 100,
  "overrides": [{
      "files": "*.md",
      "options": {
        "tabWidth": 4
      }
    },
    {
      "files": "*.json",
      "options": {
        "tabWidth": 2
      }
    }
  ]
}